<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>Login</servlet-name>
		<servlet-class>edu.uwm.cs361.LoginServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Logout</servlet-name>
		<servlet-class>edu.uwm.cs361.LogoutServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>AddUser</servlet-name>
		<servlet-class>edu.uwm.cs361.AddUserServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>CreateCourse</servlet-name>
		<servlet-class>edu.uwm.cs361.CreateCourseServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Schedule</servlet-name>
		<servlet-class>edu.uwm.cs361.ScheduleViewServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>EditInfo</servlet-name>
		<servlet-class>edu.uwm.cs361.EditInfoServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Logout</servlet-name>
		<url-pattern>/logout</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>AddUser</servlet-name>
		<url-pattern>/add-user</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>CreateCourse</servlet-name>
		<url-pattern>/create-course</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Schedule</servlet-name>
		<url-pattern>/scheduleview</url-pattern>
	</servlet-mapping>	
	<servlet-mapping>
		<servlet-name>EditInfo</servlet-name>
		<url-pattern>/edit-info</url-pattern>
	</servlet-mapping>	
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
