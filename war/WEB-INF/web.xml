<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>AddUser</servlet-name>
		<servlet-class>servlets.AddUserServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>AssignProf</servlet-name>
		<servlet-class>servlets.AssignProfServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>AssignTA</servlet-name>
		<servlet-class>servlets.AssignTAServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>EditInfo</servlet-name>
		<servlet-class>servlets.EditInfoServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>EditCourse</servlet-name>
		<servlet-class>servlets.EditCourseServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Forgot</servlet-name>
		<servlet-class>servlets.ForgotServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Login</servlet-name>
		<servlet-class>servlets.LoginServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Logout</servlet-name>
		<servlet-class>servlets.LogoutServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Proxy</servlet-name>
		<servlet-class>servlets.ProxyServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Schedule</servlet-name>
		<servlet-class>servlets.ScheduleViewServlet</servlet-class>
	</servlet>
	 <servlet>
    <servlet-name>ServeImage</servlet-name>
    <servlet-class>servlets.ServeImage</servlet-class>
  </servlet>
	<servlet>
		<servlet-name>UploadImage</servlet-name>
		<servlet-class>servlets.UploadImage</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>UserSearch</servlet-name>
		<servlet-class>servlets.UserSearchServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ViewCourses</servlet-name>
		<servlet-class>servlets.ViewCoursesServlet</servlet-class>
	</servlet>
	<!-- end servlets -->
	
	
	<servlet-mapping>
		<servlet-name>AddUser</servlet-name>
		<url-pattern>/add-user</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>AssignProf</servlet-name>
		<url-pattern>/assign-prof</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>AssignTA</servlet-name>
		<url-pattern>/assign-ta</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>EditInfo</servlet-name>
		<url-pattern>/edit-info</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>EditCourse</servlet-name>
		<url-pattern>/edit-course</url-pattern>
	</servlet-mapping>		
	<servlet-mapping>
		<servlet-name>Forgot</servlet-name>
		<url-pattern>/forgot</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Logout</servlet-name>
		<url-pattern>/logout</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Proxy</servlet-name>
		<url-pattern>/refresh-courses</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Schedule</servlet-name>
		<url-pattern>/schedule-view</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ServeImage</servlet-name>
		<url-pattern>/serve</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
    	<servlet-name>UploadImage</servlet-name>
    	<url-pattern>/upload</url-pattern>
  	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>UserSearch</servlet-name>
		<url-pattern>/user-search</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ViewCourses</servlet-name>
		<url-pattern>/view-courses</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
